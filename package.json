{
  "name": "unraid-file-server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun --bun run index.ts",
    "docker": "bun run docker:build && bun run docker:push",
    "docker:local": "docker build -t unraid-file-server:latest .",
    "docker:build": "docker build -t 192.168.68.50:5000/unraid-file-server:latest --platform=linux/amd64 .",
    "docker:push": "docker push 192.168.68.50:5000/unraid-file-server:latest",
    "check": "biome check",
    "check:fix": "biome check --write .",
    "lint": "biome lint",
    "lint:fix": "biome lint --write .",
    "format": "biome format",
    "format:fix": "biome format --write ."
  },
  "devDependencies": {
    "@qodestack/biome-config": "^2.2.0",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@qodestack/utils": "^2.23.0",
    "hono": "^4.9.5"
  }
}
